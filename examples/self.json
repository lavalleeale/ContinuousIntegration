[
  {
    "id": "assets",
    "image": "base",
    "steps": [
      "yarn --immutable",
      "yarn build"
    ],
    "uploads": [
      "/repo/assets/output"
    ]
  },
  {
    "id": "go",
    "image": "base",
    "steps": [
      "mv /neededFiles/output assets/output",
      "go build .",
      "ls -lah ."
    ],
    "neededFiles": [
      "assets:/repo/assets/output"
    ],
    "needs": [
      "assets"
    ]
  }
]